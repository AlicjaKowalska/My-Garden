<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mygarden">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="My Garden"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyGarden">
        <activity android:name=".PlantInfo"></activity>
        <activity android:name=".Edit" />
        <activity android:name=".Plants" />
        <activity android:name=".Add" />
        <activity
            android:name=".Settings"
            android:label="" />
        <activity android:name=".LoadingScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Harmonogram" />

        <receiver android:name=".ReminderBroadcast"/>
        <receiver android:name=".TaskBroadcast"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>